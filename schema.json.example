{
  "outputLocation": "./output.json",
  "hierarchy": [
    {
      "class": "artboard",
      "layers": [
        {
          "class": "group",
          "pattern": "^\\w+(-\\w+)*$",
          "count": "1",
          "layers": [
            {
              "pattern": "^100$",
              "count": "1",
              "assert": {
                "rotation": "0",
                "width": "220",
                "height": "80",
                "isVisible": "true"
              },
              "output": ["fills.color"]
            },
            {
              "pattern": "^200$",
              "count": "1",
              "output": ["fills.color"]
            },
            {
              "pattern": "^300$",
              "count": "1",
              "output": ["fills.color"]
            },
            {
              "pattern": "^400$",
              "count": "1",
              "output": ["fills.color"]
            },
            {
              "pattern": "^500$",
              "count": "1",
              "output": ["fills.color"]
            },
          ]
        }
      ]
    }
  ]
}
